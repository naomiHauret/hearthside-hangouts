{
  "expo": {
    "name": "Hearthside Hangouts",
    "slug": "hearthsidehangouts",
    "scheme": "hearthsidehangouts",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hearthsidehangouts.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.hearthsidehangouts.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "naomihauret",
    "extra": {
      "eas": {
        "projectId": "11e2c5bc-21d0-4201-907d-1eae3a6c746a"
      }
    },
    "plugins": ["@config-plugins/react-native-webrtc", "expo-localization"]
  }
}
